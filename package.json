{
  "name": "jmahc.github.io",
  "version": "0.1.0",
  "author": "Jordan McArdle <jordan@mcardle.tech> (http://mcardle.tech/)",
  "description": "http://mcardle.tech/",
  "homepage": "https://github.com/jmahc/jmahc.github.io#readme",
  "main": "src/index.js",
  "files": ["dist", "dll"],
  "scripts": {
    "build": "rimraf ./dist/* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "clean": "rm -rf ./dist/* ./dll/* node_modules yarn.lock .yarnclean yarn-error.log ./**/*.DS_STORE ./.DS_STORE",
    "clean-all": "yarn run clean",
    "clean-dist": "rm -rf ./dist/*",
    "clean-dll": "del /q dll\\*",
    "clean-yarn": "rm -rf node_modules yarn.lock .yarnclean yarn-error.log ./**/*.DS_STORE ./.DS_STORE",
    "dev": "rm -rf ./dll/* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "format": "prettier-standard 'src/**/*.js' && prettier-standard 'src/**/*.jsx'",
    "postmerge": "yarn",
    "precommit": "lint-staged",
    "release": "standard-version",
    "start": "cross-env NODE_ENV=development PORT_NUMBER=8080 webpack-dev-server --profile --progress --env development --open",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "compression-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.5",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "generact": "^0.3.1",
    "gh-pages": "1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "inline-chunk-manifest-html-webpack-plugin": "^1.1.1",
    "lint-staged": "^4.2.1",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss": "^6.0.11",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "preload-webpack-plugin": "^2.0.0",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.0",
    "prettier-standard": "^7.0.1",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "redbox-react": "^1.5.0",
    "remarkable": "^1.7.1",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "engines": {
    "node": ">=8.5.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "keywords": ["react", "webpack"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/jmahc/jmahc.github.io/"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": ["prettier-standard", "git add"],
      "src/**/*.jsx": ["prettier-standard", "git add"]
    }
  }
}
