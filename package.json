  "scripts": {
    "build":
      "rm -rf ./dist/* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "clean":
      "rm -rf ./dist/* ./dll/* node_modules yarn.lock .yarnclean yarn-error.log ./**/*.DS_STORE ./.DS_STORE",
    "format": "prettier-standard 'src/**/*.js'",
    "postinstall":
      "cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "postmerge": "yarn",
    "precommit": "lint-staged",
    "release": "standard-version",
    "start":
      "cross-env NODE_ENV=development PORT_NUMBER=8080 webpack-dev-server --profile --progress --env development --open",
    "webpack-defaults": "webpack-defaults"
  },
